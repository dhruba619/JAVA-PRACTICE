import java.util.Enumeration;
import java.util.Hashtable;


public class HashTable {
	public static void main(String[] args){
		String str;
		Hashtable paybackPoints = new Hashtable();
		paybackPoints.put("Spiderman", 30.40);
		Enumeration<E> data;
		data = paybackPoints.keys();
		while(data.hasMoreElements()){
			str = data.nextElement();
			System.out.println(str+": "+paybackPoints.get(str));
		}
	}
}
