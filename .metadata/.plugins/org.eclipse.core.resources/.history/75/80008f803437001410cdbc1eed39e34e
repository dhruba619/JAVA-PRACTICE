import java.util.Enumeration;
import java.util.Hashtable;


public class HashTable {
	public static void main(String[] args){
		Hashtable points = new Hashtable();
		Enumeration data;
		points.put("TonyStark", 700);
		points.put("BruceWayne", 600);
		points.put("BruceBaner", 659);
		points.put("ClerkKent", "SuperMan");
		data = points.keys();
		while(data.hasMoreElements()){
			String str = (String)data.nextElement();
			System.out.println(str+": "+points.get(str));
		}
	}
}
