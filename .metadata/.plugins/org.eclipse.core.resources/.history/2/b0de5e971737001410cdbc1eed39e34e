
public class BubbleSortTry {
	public static void main(String[] args){
		int[] arr = {12,45,663,98,15};
		for (int count =1;count<=arr.length-1;count++){}
			for (int count2= 0;count2<=arr.length;count2++){
				if(arr[count2]>arr[count2+1]){
					int temp =arr[count2];
					arr[count2]=arr[count2+1];
					arr[count2+1]=temp;
					         
				}
				
			}
			for(int count =0;count<arr.length;count++){
				 System.out.println(" "+arr[count]);
				}
	}
}
